{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "techList": ["VDSL", "ISDN", "xDSL"],
  "deviceList": ["DSLAM", "MDU", "DPU", "DPO", "ONT"],
  "upstream": [10, 50],
  "downstream": [100, 500]
}
